{
  "name": "project-name",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "start:prod": "node -r newrelic dist/main.js",
    "start:debug": "nodemon --config nodemon-debug.json",
    "sonar": "node sonar-project.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/e2e/jest-e2e.json",
    "test:e2e:watch": "jest --watch --config ./test/e2e/jest-e2e.json",
    "gcp-build": "tsc -p tsconfig.build.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck"
  },
  "dependencies": {
    "@nestjs/cache-manager": "2.3.0",
    "@nestjs/common": "10.4.22",
    "@nestjs/config": "3.3.0",
    "@nestjs/core": "10.4.22",
    "@nestjs/microservices": "10.4.22",
    "@nestjs/platform-express": "10.4.22",
    "@nestjs/swagger": "7.4.2",
    "@nestjs/typeorm": "10.0.2",
    "cache-manager": "5.7.6",
    "cache-manager-redis-store": "3.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.3",
    "cookie-parser": "1.4.7",
    "express-session": "1.19.0",
    "mysql2": "3.16.3",
    "newrelic": "13.12.0",
    "reflect-metadata": "0.1.14",
    "rimraf": "3.0.2",
    "rxjs": "7.8.1",
    "swagger-ui-express": "5.0.1",
    "typeorm": "0.3.28"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.9",
    "@nestjs/schematics": "10.2.3",
    "@nestjs/testing": "10.4.22",
    "@types/cookie-parser": "1.4.10",
    "@types/express": "4.17.21",
    "@types/express-session": "1.18.2",
    "@types/jest": "29.5.14",
    "@types/node": "20.19.32",
    "@types/supertest": "2.0.16",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "better-sqlite3": "12.6.2",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-prettier": "5.5.5",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-sonar-reporter": "2.0.0",
    "nodemon": "3.1.11",
    "prettier": "3.8.1",
    "run-script-webpack-plugin": "0.2.3",
    "sonarqube-scanner": "3.5.0",
    "supertest": "6.3.4",
    "ts-jest": "29.4.6",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3",
    "webpack": "5.105.2",
    "webpack-node-externals": "3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/main.ts",
      "!src/app.module.ts",
      "!src/**/*.module.ts"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/main.ts",
      "/src/(.*).module.ts",
      "/src/(.*)/(.*).(module|constans|repository).ts",
      "/src/(.*)/(dtos|entities)",
      "/src/decorators/(.*)",
      "/src/filters/http-exception.filter.ts",
      "/src/interceptors/logging.interceptor.ts",
      "/src/middlewares/roles.middleware.ts",
      "/src/entity/entity.controller.ts"
    ],
    "testResultsProcessor": "jest-sonar-reporter",
    "coverageReporters": [
      "text",
      "lcov",
      "cobertura"
    ]
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "resolutions": {
    "@types/express": "4.17.21",
    "@types/express-serve-static-core": "4.19.0",
    "@types/serve-static": "1.15.5"
  },
  "packageManager": "yarn@4.12.0"
}
